{
  "name": "zod2md",
  "version": "0.0.1",
  "description": "Generate Markdown docs from Zod schemas",
  "license": "MIT",
  "author": "MatÄ›j Chalk <matejchalk@gmail.com>",
  "keywords": [
    "markdown",
    "zod",
    "cli"
  ],
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/matejchalk/zod2md.git"
  },
  "homepage": "https://github.com/matejchalk/zod2md#readme",
  "bugs": {
    "url": "https://github.com/matejchalk/zod2md/issues"
  },
  "type": "module",
  "files": [
    "dist/"
  ],
  "bin": "dist/bin.js",
  "types": "dist/index.d.ts",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "prepublishOnly": "npm test && npm run build",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "e2e": "vitest run --config e2e/vitest.config.ts",
    "e2e:watch": "vitest watch --config e2e/vitest.config.ts",
    "build": "tsup"
  },
  "dependencies": {
    "@commander-js/extra-typings": "^12.0.0",
    "bundle-require": "^4.0.2",
    "commander": "^12.0.0",
    "esbuild": "^0.19.11"
  },
  "peerDependencies": {
    "zod": "^3.22.0"
  },
  "devDependencies": {
    "@types/node": "^20.11.0",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.2",
    "zod": "^3.22.4"
  }
}
